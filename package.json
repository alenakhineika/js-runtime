  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "compile": "npm run antlr4-js && npm run antlr4-cs",
    "antlr4-js": "java -Xmx500M -cp '/usr/local/lib/antlr-4.7.1-complete.jar:$CLASSPATH' org.antlr.v4.Tool  -Dlanguage=JavaScript -o lib -visitor ECMAScript.g4",
    "antlr4-cs": "java -Xmx500M -cp '/usr/local/lib/antlr-4.7.1-complete.jar:$CLASSPATH' org.antlr.v4.Tool  -Dlanguage=JavaScript -o lib -visitor CSharpLexer.g4 CSharpParser.g4",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
